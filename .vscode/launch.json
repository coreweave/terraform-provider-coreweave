{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Terraform Provider Server",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/",
      "env": {
        "CGO_ENABLED": "0"
      },
      "args": [
        "--debug"
      ],
      "envFile": "${workspaceFolder}/__debug.env",
      "internalConsoleOptions": "openOnSessionStart",
      "showLog": true
    }
  ]
}
